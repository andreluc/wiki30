<guid>24ec5979-2510-4e3c-a0d5-a42ef43b0b05</guid>
<property>
<code>{{velocity}}
##
## List all the workspaces.
##
$xwiki.ssx.use('Main.Spaces')##
#set($workspaceList = $xwiki.queryManager.xwql("select wikiServerDescriptor.server, wikiServerDescriptor.wikiprettyname from Document workspaceDescriptorDocument, Document wikiServerDescriptorDocument, workspaceDescriptorDocument.object(WorkspaceManager.WorkspaceClass) as workspaceDescriptor, wikiServerDescriptorDocument.object(XWiki.XWikiServerClass) as wikiServerDescriptor where workspaceDescriptor.name = wikiServerDescriptorDocument.fullName").execute())
{{html wiki="true"}}
&lt;ul class="xlist"&gt;
  ##
  ## List workspaces.
  ##
  #foreach($workspace in $workspaceList)    
      &lt;li class="xitem xunderline xhighlight space"&gt;
        &lt;div class="xitemcontainer"&gt;
          &lt;div class="spSpaceName"&gt;{{html}}&lt;a href="$services.workspaceManager.getWorkspace($workspace.get(0)).wikiDescriptor.homePageUrl"&gt;$workspace.get(1)&lt;/a&gt;{{/html}}&lt;/div&gt;  
          &lt;div class="xspacer"&gt;&amp;nbsp;&lt;/div&gt;
        &lt;/div&gt;
      &lt;/li&gt;    
  #end
## Space creation.
  ##
  ## Only if the current user has admin rights and the current action is not inline. If it's inline, then there is already a form, and if we render the second, nested one, the DOM is messed up in IE.
  #if($xwiki.hasAdminRights() &amp;&amp; "$!context.action" != "inline")
    &lt;li id="spSpaceCreateItem" class="xitem space"&gt;
      &lt;div class="xitemcontainer"&gt;
        &lt;div id="spSpaceCreateLabel" class="spSpaceName"&gt;&lt;a href="{{html wiki="false"}}$xwiki.getURL('WorkspaceManager.CreateNewWorkspace', 'view'){{/html}}" &gt;$msg.get('workspacemanager.createworkspace')&lt;/a&gt;&lt;/div&gt;
        &lt;div class="spacer"&gt;&amp;nbsp;&lt;/div&gt;        
      &lt;/div&gt;
    &lt;/li&gt;
  #end

&lt;/ul&gt;
{{/html}}
{{/velocity}}</code>
