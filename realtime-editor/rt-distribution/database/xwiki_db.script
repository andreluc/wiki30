INSERT INTO XWIKIRCS VALUES(1940724201,1,1,'2009-09-09 01:00:00.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>Panels</web>\u000a<name>CreatePanel</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>Panels.WebHome</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1252454400000</creationDate>\u000a<date>1252454400000</date>\u000a<contentUpdateDate>1252454400000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title>$msg.get("xe.panels.create")</title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/2.0</syntaxId>\u000a<hidden>false</hidden>\u000a<content>{{velocity}}\u000a#macro(displayCreatePanelForm)\u000a{{html}}\u000a  &lt;form method="post" action="$doc.getURL(''view'', ''xpage=plain'')" onsubmit="cancelCancelEdit()"&gt;\u000a    &lt;div&gt;\u000a      &lt;input type="hidden" name="form_token" value="$!{services.csrf.getToken()}" /&gt;\u000a      &lt;input type="hidden" name="create" value="1"/&gt;\u000a      &lt;label for="panelTitle" class="hidden"&gt;$msg.get("xe.panels.create.title")&lt;/label&gt;\u000a      &lt;input type="text" id="panelTitle" name="panelTitle" value="$msg.get("xe.panels.create.title")" class="panelinput" onfocus="if(this.value==''$msg.get("xe.panels.create.title")'') this.value=''''" onblur="if(this.value=='''') this.value=''$msg.get("xe.panels.create.title")''"/&gt;\u000a      &lt;input type="submit" class="button" value="$msg.get("create")"/&gt;\u000a    &lt;/div&gt;\u000a  &lt;/form&gt;\u000a{{/html}}##\u000a#end\u000a#macro(createPanel $title)\u000a  #set($docName=$title.replaceAll('':'', ''%3A''))\u000a  #set($displayTitle = $title)\