$errorcode == -2)\u000a      {{warning}}$msg.get(''search.admin.lucene.indexing.message.alreadystarted''){{/warning}}\u000a    #end\u000a  #end\u000a#else## !hasAdmin\u000a {{error}}$msg.get(''needadminrights'').{{/error}}\u000a#end## hasAdmin\u000a{{/velocity}}\u000a\u000a(% id="Hsearch.admin.lucene.status.title" %)\u000a== {{velocity}}$msg.get(''search.admin.lucene.status.title''){{/velocity}} ==\u000a\u000a|={{velocity}}$msg.get(''search.admin.lucene.status.infotitle''){{/velocity}}|={{velocity}}$msg.get(''search.admin.lucene.status.valuetitle''){{/velocity}}\u000a|{{velocity}}$msg.get(''search.admin.lucene.status.indexed''){{/velocity}}|{{velocity}}$lucene.luceneDocCount{{/velocity}}\u000a|{{velocity}}$msg.get(''search.admin.lucene.status.indexing''){{/velocity}}|{{velocity}}$lucene.queueSize{{/velocity}}\u000a\u000a(% id="Hsearch.admin.lucene.indexing.title" %)\u000a== {{velocity}}$msg.get(''search.admin.lucene.indexing.title''){{/velocity}} ==\u000a\u000a{{velocity}}$msg.get(''search.admin.lucene.indexing.description''){{/velocity}}\u000a\u000a{{velocity}}\u000a#if ($hasAdmin)\u000a{{html}}\u000a<form action="$xwiki.requestURL" enctype="multipart/form-data" method="post">\u000a<fieldset>\u000a<input type="hidden" name="form_token" value="$!{services.csrf.getToken()}" />\u000a\u000a  ## Does not really make sense to reindex the whole farm from a subwiki\u000a  #if ($xcontext.isMainWiki())\u000a<p>\u000a    <label for="indexfarm" class="hidden">$msg.get(''search.admin.lucene.indexing.action.indexfarm'')</label><input id="indexfarm" name="action" type="radio" value="indexfarm" #if (!$request.action || $request.action == ''indexfarm'')checked="checked"#end/>$msg.get(''search.admin.lucene.indexing.action.indexfarm'')\u000a</p>\u000a  #end\u000a\u000a<p>\u000a  <label for="indexcurrentwiki" class="hidden">$msg.get(''search.admin.lucene.indexing.action.indexcurrentwiki'')</label><input id="indexcurrentwiki" name="action" type="radio" value="indexcurrentwiki" #if((!$xcontext.isMainWiki() && !$request.action) || $request.action == ''indexcurrentwiki''))checked="checked"#end/>$msg.get(''search.admin.lucene.indexing.action.indexcurrentwiki'')\u000a</p>\u000a\u000a  ## Needs programming right because it takes a custom HQL query which could be dangerous\u000a<p>\u000a  <label for="indexcustom" class="hidden">$msg.get(''search.admin.lucene.indexing.action.indexcustom'')</label><input id="indexcustom" name="action" type="radio" value="indexcustom" #if ($request.action == ''indexcustom'')checked="checked"#end/>$msg.get(''search.admin.lucene.indexing.action.indexcustom'')\u000a</p>\u000a\u000a<p>\u000a  #if ($xcontext.isMainWiki())\u000a    <label for="indexcustom_wikis">$msg.get(''search.admin.lucene.indexing.action.indexcustom.wikis'')</label> <input id="indexcustom_wikis" name="wikis" type="text" title="$escapetool.xml($msg.get(''search.admin.lucene.indexing.action.indexcustom.wikis.title''))" value="$!wikisString" />\u000a  #else\u000a    <input id="indexcustom_wikis" name="wikis" value="${xcontext.database}" type="hidden" value="$!wikisString" />\u000a  #end\u000a</p>\u000a\u000a  #if ($hasProgramming && $xcontext.isMainWiki())\u000a<p>\u000a    <label for="indexcustom_hqlFilter">$msg.get(''search.admin.lucene.indexing.action.indexcustom.hqlfilter'')</label> <input id="indexcustom_hqlFilter" name="hqlFilter" type="text" title="$escapetool.xml($msg.get(''search.admin.lucene.indexing.action.indexcustom.hqlfilter.title''))" #if($request.hqlFilter)value="${escapetool.xml($request.hqlFilter)}"#end />\u000a</p>\u000a  #end\u000a\u000a<p>\u000a  <label for="indexcustom_clearIndex">$msg.get(''search.admin.lucene.indexing.action.indexcustom.clearindex'')</label> <input id="indexcustom_clearIndex" name="clearIndex" type="checkbox" title="$escapetool.xml($msg.get(''search.admin.lucene.indexing.action.indexcustom.clearindex.title''))" #if($clearIndex)checked="checked"#end />\u000a</p>\u000a<p>\u000a  <label for="indexcustom_onlyNew">$msg.get(''search.admin.lucene.indexing.action.indexcustom.onlynew'')</label> <input id="indexcustom_onlyNew" name="onlyNew" type="checkbox" title="$escapetool.xml($msg.get(''search.admin.lucene.indexing.action.indexcustom.onlynew.title''))" #if($onlyNew)checked="checked"#end />\u000a</p>\u000a\u000a<span class="buttonwrapper"><input type="submit" value="$msg.get(''search.admin.lucene.indexing.button'')" class="button"/></span>\u000a\u000a</fieldset>\u000a</form>\u000a{{/html}}\u000a#else## !$hasAdmin\u000a  {{error}}$msg.get(''needadminrights'').{{/error}}\u000a#end\u000a{{/velocity}}','1.1','','SearchAdmin',NULL,0,'','','',FALSE,'xwiki/2.0',FALSE)
