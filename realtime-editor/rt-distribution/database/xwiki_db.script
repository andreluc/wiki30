INSERT INTO XWIKILARGESTRINGS VALUES(606388356,'code','{{velocity}}\u000a##\u000a## List spaces within the wiki.\u000a##\u000a$xwiki.ssx.use("Main.Spaces")\u000a#set($spaceList = $xwiki.getSpaces())\u000a{{html wiki="true"}}\u000a<ul class="xlist">\u000a  ##\u000a  ## List spaces.\u000a  ##\u000a  #foreach($space in $spaceList)\u000a    ## Display space only if the user has view right on space home and if the space is not blacklisted.\u000a    ## $blacklistedSpaces is set in xwikivars.vm\u000a    #if($xwiki.hasAccessLevel(''view'', "${space}.WebHome") && !$blacklistedSpaces.contains($space))\u000a      <li class="xitem xunderline xhighlight space">\u000a        <div class="xitemcontainer">        \u000a          <div class="spSpaceName">[[${space}>>${space}.WebHome]]</div>\u000a          <div class="xshowonhover spSpaceActions">\u000a            ## Link to the space index.\u000a            <a href="$xwiki.getURL(''Main.SpaceIndex'', ''view'', "space=$escapetool.url($space)")" title="$msg.get(''xe.spaces.action.index'')">\u000a              <img src="$xwiki.getSkinFile(''icons/silk/book_open.gif'')" alt="Space index icon"/>\u000a            </a>\u000a            ## If the user has admin right on the space display links to administration and delete.\u000a            #if($xwiki.hasAccessLevel(''admin'', "${space}.XWikiPreferences"))  \u000a              <a href="$xwiki.getURL("${space}.WebPreferences", ''admin'')" title="$msg.get(''xe.spaces.action.admin'')">\u000a                <img src="$xwiki.getSkinFile(''icons/silk/cog.gif'')" alt="Administrate space icon"/>\u000a              </a>                  \u000a            #end          \u000a          </div> \u000a          <div class="xspacer">&nbsp;</div>\u000a        </div>   \u000a      </li>\u000a    #end    \u000a  #end\u000a  ##\u000a  ## Space creation.\u000a  ##\u000a  #if($xwiki.hasAdminRights())  \u000a    <li id="spSpaceCreateItem" class="xitem space">      \u000a      <div class="xitemcontainer">\u000a        <div id="spSpaceCreateLabel" class="spSpaceName"><a href="$xwiki.getURL(''XWiki.CreatePage'', ''view'', ''tocreate=space'')" onclick="toggleClass($(''spSpaceCreateForm''), ''hidden''); return false;">$msg.get(''xe.spaces.createspace'')</a></div>\u000a        <div class="spacer">&nbsp;</div>        \u000a        <div id="spSpaceCreateForm" class="hidden">\u000a          <form method="post" action="$doc.getURL(''view'', ''xpage=create'')">\u000a            <div>\u000a              <input name="tocreate" type="hidden" value="space" />       \u000a              #set($newSpaceName = $msg.get(''xe.spaces.createspace.defaultname''))\u000a              <label class="hidden" for="spSpaceCreateTextInput">$newSpaceName</label>\u000a              <input id="spSpaceCreateTextInput" type="text" name="space" value="$newSpaceName" class="panelinput" onfocus="if(this.value==''$newSpaceName'') this.value=''''" onblur="if(this.value=='''') this.value=''$newSpaceName''" />        \u000a              <span class="buttonwrapper">\u000a                <input type="submit" value="$msg.get(''xe.spaces.createspace.submit'')" class="button" />\u000a              </span>\u000a            </div>\u000a          </form>\u000a        </div>\u000a        <div class="spacer">&nbsp;</div>\u000a      </div>     \u000a    </li>\u000a  #end\u000a</ul>\u000a{{/html}}\u000a{{/velocity}}\u000a')
