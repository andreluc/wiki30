INSERT INTO XWIKIDOC VALUES(1244697757,'XWiki.AdminInlineSheet','AdminInlineSheet','AdminInlineSheet','','',0,'2008-06-10 09:53:35.000000000','2008-06-19 17:25:23.000000000','2008-06-06 11:18:18.000000000','XWiki.Admin','XWiki.Admin','XWiki.Admin','XWiki','###----------------------------------\u000a## modified version of editinline.vm\u000a###----------------------------------\u000a#if ($doc.fullName != ''XWiki.AdminInlineSheet'') ## Prevents infinite loops\u000a#set($formname = ''inline'')\u000a#set($saveaction = ''save'')\u000a#set($previewenabled = true)\u000a#set($xnotification = $!request.getParameter(''xnotification''))\u000a<form id="inline" method="post" action="$doc.getURL(''preview'')">\u000a  <div>\u000a    <div class="hidden"><input type="hidden" name="xcontinue" value="$doc.getURL(''admin'', "${request.queryString}&editmode=inline").replaceAll(''&'',''&amp;'')" /></div>\u000a    <input type="hidden" name="xredirect" value="$doc.getURL(''admin'', "editor=${editor}&section=${section}&space=XWiki").replaceAll(''&'',''&amp;'')" />\u000a    <input type="hidden" name="form_token" value="$!{services.csrf.getToken()}" />\u000a    <input type="hidden" name="xnotification" value="$!xnotification" />\u000a    <input type="hidden" name="template" value="$!request.template" />\u000a    <input type="hidden" name="language" value="$!doc.language" />\u000a    #if("$!request.parent" != '''')\u000a    <input type="hidden" name="parent" value="$!request.parent" />\u000a    #end\u000a    #if("$!request.title" != "")\u000a    <input type="hidden" name="title" value="$!request.title" />\u000a    #end\u000a    #set($force = $!request.get(''force''))\u000a    #if (($doc.getLocked() == true) && (!$force)) ## locking\u000a    <div>\u000a     $msg.get(''doclockedby'') $xwiki.getLocalUserName($doc.getLockingUser()). <br />\u000a     <a href="$doc.getURL(''inline'', "$!request.getQueryString()&force=1")">$msg.get(''forcelock'')</a>\u000a    </div>\u000a    #else\u000a      #foreach