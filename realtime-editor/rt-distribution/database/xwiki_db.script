INSERT INTO XWIKIRCS VALUES(-434867184,1,1,'2010-05-04 17:21:21.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>XWiki</web>\u000a<name>Results</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>Main.WebSearch</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1252454400000</creationDate>\u000a<date>1272990081000</date>\u000a<contentUpdateDate>1272990081000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title>$msg.get("search.page.results")</title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/2.0</syntaxId>\u000a<hidden>false</hidden>\u000a<content>{{velocity}}\u000a{{html}}\u000a#set($showdata = 0)\u000a&lt;div id="searchResults"&gt;\u000a  #foreach ($item in $list)\u000a    #set($itemClass = "$item.class")\u000a    #if($itemClass.contains(''lucene''))\u000a      #set($isLuceneResult = true)\u000a    #else\u000a      #set($isLuceneResult = false)\u000a    #end\u000a    #if($isLuceneResult)\u000a      #set($itemfullname = "${item.wiki}:${item.space}.${item.name}")\u000a    #else\u000a      #set($itemfullname = "${xcontext.database}:${item}")\u000a    #end\u000a    #if ($xwiki.hasAccessLevel(''view'', $xcontext.user, $itemfullname))\u000a      &lt;div class="resultItem"&gt;\u000a      &lt;div class="resultContainer"&gt;\u000a      #if($isLuceneResult)\u000a        #set($itemDoc = $item)\u000a      #else\u000a        #set($itemDoc = $xwiki.getDocument($item))\u000a      #end\u000a      #set($author = $xwiki.getUserName($itemDoc.author))\u000a      ##*********************************************************##\u000a      ## Description                                             ##\u000a      ##*********************************************************##\u000a      &lt;div class="itemDescription type-${item.type}"&gt;\u000a        ##*********************************************************##\u000a        ## Title                                                   ##\u000a        ##*********************************************************##\u000a        &lt;h2 class="itemTitle"&gt;\u000a          #if($isLuceneResult)\u000a            #if($item.type == ''attachment'')\u000a              &lt;a href="$xwiki.getDocument($itemfullname).getAttachmentURL($item.filename)"&gt;$xwiki.getXMLEncoded($item.filename)&lt;/a&gt;\u000a            #else\u000a              &lt;a href="$xwiki.getURL($itemfullname)"&gt;$xwiki.getXMLEncoded($xwiki.getDocument($itemfullname).getDisplayTitle())&lt;/a&gt;\u000a            #end\u000a          #else\u000a            &lt;a href="$xwiki.getURL($itemfullname)"&gt;$xwiki.getXMLEncoded($itemDoc.getDisplayTitle())&lt;/a&gt;\u000a          #end\u000a        &lt;/h2&gt;\u000a        ##*********************************************************##\u000a        ## Location                                                ##\u000a        ##*********************************************************##\u000a        &lt;div class="itemLocation"&gt;\u000a          $msg.get(''search.item.location'', [\u000a          $xwiki.getXMLEncoded($itemDoc.wiki), $xwiki.getURL("${itemDoc.wiki}:Main.WebHome"),\u000a          $xwiki.getXMLEncoded($itemDoc.space), $xwiki.getURL("${itemDoc.wiki}:${itemDoc.space}.WebHome"),\u000a          $xwiki.getXMLEncoded($itemDoc.name), $xwiki.getURL($itemfullname)])\u000a        &lt;/div&gt;\u000a        ##*********************************************************##\u000a        ## Modifiers - content update date                         ##\u000a        ##*********************************************************##\u000a        &lt;div class="itemModified"&gt;\u000a          #if ($author == ''XWikiGuest'')\u000a            #set($author = $msg.get(''search.page.results.guest''))\u000a          #end\u000a          $msg.get(''search.item.modified'', [$author, $xwiki.formatDate($itemDoc.date, ''dd MMM yyyy'')])\u000a        &lt;/div&gt;\u000a## TODO: Uncomment when the attachment and comment count will be implemented\u000a##         &lt;div class="itemOthers"&gt;\u000a          ##*********************************************************##\u000a          ## Rating                                                  ##\u000a          ##*********************************************************##\u000a          #if($xwiki.ratings &amp;&amp; $item.type != ''attachment'')\u000a            #set($avarageRating = $xwiki.ratings.getAverageRating($xwiki.getDocument($itemfullname)).averageVote)\u000a            #set($percentageRating = $avarageRating * 20)\u000a            &lt;span class="itemRating" title="$msg.get(''search.item.rating.title'')"&gt;\u000a              &lt;span class="currentRating" style="width:$percentageRating%;"&gt;&lt;/span&gt;\u000a            &lt;/span&gt;\u000a          #end\u000a##         &lt;/div&gt;\u000a      &lt;/div&gt; ## end &lt;div class="itemDescription"&gt;\u000a      #if($isScored)\u000a        #set($resval = $itemDoc.score * 100)\u000a        ##*********************************************************##\u000a        ## Relevance                                               ##\u000a        ##*********************************************************##\u000a        &lt;div class="itemRelevance"&gt;\u000a          &lt;span class="relevance" title="$msg.get(''search.item.relevance.title'')"&gt;\u000a            &lt;span class="currentRelevance" style="width:${resval}%;"&gt;&lt;/span&gt;\u000a          &lt;/span&gt;\u000a          &lt;span class="relevanceText"&gt;$resval.intValue()%&lt;/span&gt;\u000a        &lt;/div&gt;\u000a      #end ## isScored\u000a      ##*********************************************************##\u000a      ## Actions (admins only)                                   ##\u000a      ##*********************************************************##\u000a#*\u000a      #if($xwiki.hasAdminRights())\u000a          #set($copyURL = $xwiki.getURL(''XWiki.CopyDocument'', ''view'', "sourcedoc=${util.encodeURI($itemfullname)}"))\u000a          #set($deleteURL = $xwiki.getDocument($itemfullname).getURL(''delete''))\u000a          #set($renameURL = $xwiki.getDocument($itemfullname).getURL(''view'', ''xpage=rename&amp;amp;step=1''))\u000a          #set($rightsURL = $xwiki.getDocument($itemfullname).getURL(''edit'', ''editor=rights''))\u000a          &lt;div class="itemActions"&gt;\u000a            &lt;a href="$copyURL" class="tool copy" title="$msg.get(''search.page.results.copy'')"&gt;$msg.get(''search.page.results.copy'')&lt;/a&gt;\u000a            &lt;a href=''$renameURL'' class="tool rename" title="$msg.get(''search.page.results.rename'')"&gt;$msg.get(''search.page.results.rename'')&lt;/a&gt;\u000a            &lt;a href="$deleteURL" class="tool delete" title="$msg.get(''search.page.results.delete'')"&gt;$msg.get(''search.page.results.delete'')&lt;/a&gt;\u000a            &lt;a href=''$rightsURL'' class="tool rights" title="$msg.get(''search.page.results.rights'')"&gt;$msg.get(''search.page.results.rights'')&lt;/a&gt;\u000a          &lt;/div&gt; ## itemActions\u000a        #end ## has admin rights *# ##\u000a      &lt;/div&gt; ## resultContainer\u000a      &lt;/div&gt; ## resultItem\u000a    #end ## hasAccessLevel\u000a  #end ## foreach item\u000a&lt;/div&gt; ## searchResults\u000a{{/html}}\u000a{{/velocity}}</content>\u000a</xwikidoc>\u000a')
INSERT INTO XWIKIRCS VALUES(-419313613,1,1,'2010-02-19 18:48:36.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>Main</web>\u000a<name>WebRss</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>Main.RssFeeds</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1252450800000</creationDate>\u000a<date>1266601716000</date>\u000a<contentUpdateDate>1266601716000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title>$msg.get("xe.rss.pages.modified")</title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/2.0</syntaxId>\u000a<hidden>false</hidden>\u000a<content>{{velocity}}\u000a## Backward compatibility redirect, if the RSS feed is called without the outputSyntax query argument, force it.\u000a#if (("$!request.xpage" == ''plain'' || "$!request.xpage" == ''rdf'') &amp;&amp; "$!request.outputSyntax" != ''plain'')\u000a  $response.sendRedirect($xwiki.getURL($doc.fullName, ''view'', ''xpage=plain&amp;outputSyntax=plain''))\u000a#end\u000a#if ("$!request.xpage" == ''plain'' &amp;&amp; "$!request.outputSyntax" == ''plain'')\u000a  $response.setContentType(''application/rss+xml'')\u000a  #if("$!{request.space}" == '''')\u000a    ## RSS feed for the whole wiki\u000a    #set ($request = ''where 1=1 order by doc.date desc'')\u000a    #set ($description = $msg.get(''xe.rss.feed.description''))\u000a  #else\u000a    ## RSS feed for a single space\u000a    #set ($request = "where doc.space=''${request.space}'' order by doc.date desc")\u000a    #set ($description = $msg.get(''xe.rss.space.description'', [$request.space]))\u000a  #end\u000a  #set ($feed = $xwiki.feed.getWebFeed($request, 20