INSERT INTO XWIKIRCS VALUES(160648099,1,1,'2010-01-07 10:22:47.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>XWiki</web>\u000a<name>XWikiUserProfileSheet</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>XWiki.XWikiUserSheet</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1262856167000</creationDate>\u000a<date>1262856167000</date>\u000a<contentUpdateDate>1262856167000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title></title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/2.0</syntaxId>\u000a<hidden>false</hidden>\u000a<content>{{velocity}}\u000a#set($obj = $doc.getObject("XWiki.XWikiUsers"))\u000a#set($obfuscateEmail = true)\u000a#if(!$obj)\u000a= $msg.get(''xe.admin.users.sheet'') =\u000a\u000a{{info}}$msg.get(''xe.admin.users.applyonusers''){{/info}}##\u000a#else\u000a$doc.use($obj)\u000a{{html clean="false" wiki="true"}}\u000a#set($username = $xwiki.getUserName($doc.fullName, false))\u000a= $msg.get(''platform.core.profile.title'', [$username]) =\u000a\u000a#if($xcontext.action == ''view'')\u000a  &lt;div class="vcard"&gt;\u000a  &lt;span class="fn hidden"&gt;$username&lt;/span&gt;\u000a#end\u000a$xwiki.ssx.use("XWiki.XWikiUserSheet")##\u000a$xwiki.ssx.use(''Main.Dashboard'')##\u000a&lt;div class="dashboard dashboard${xcontext.action}"&gt;\u000a  &lt;div class="dashboardleft"&gt;\u000a    &lt;div class="userInfo"&gt;\u000a      #if($xcontext.action == ''view'' &amp;&amp; $hasEdit)\u000a        &lt;div class="editProfileCategory"&gt;&lt;a href="$doc.getURL(''inline'', ''category=profile'')"&gt;&lt;span class="hidden"&gt;$msg.get(''platform.core.profile.category.profile.edit'')&lt;/span&gt;&lt;/a&gt;&lt;/div&gt;\u000a      #end\u000a      ## Please do not insert extra empty lines here (as it affects the validity of the rendered xhtml)\u000a      &lt;h1&gt;$msg.get(''platform.core.profile.section.personal'')&lt;/h1&gt;\u000a      &lt;dl&gt;\u000a        &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.firstname'')&lt;/label&gt;&lt;/dt&gt;\u000a        &lt;dd #if($xcontext.action == ''view'')class="given-name"#end&gt;$doc.display(''first_name'')&lt;/dd&gt;\u000a        &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.lastname'')&lt;/label&gt;&lt;/dt&gt;\u000a        &lt;dd #if($xcontext.action == ''view'')class="family-name"#end&gt;$doc.display(''last_name'')&lt;/dd&gt;\u000a        #if(($obj.getProperty(''company'') &amp;&amp; $obj.getProperty(''company'').getValue() != '''') || $xcontext.action != ''view'')\u000a          &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.company'')&lt;/label&gt;&lt;/dt&gt;\u000a          &lt;dd #if($xcontext.action == ''view'')class="org"#end&gt;$doc.display(''company'')&lt;/dd&gt;\u000a        #end\u000a        #if(($obj.getProperty(''comment'') &amp;&amp; $obj.getProperty(''comment'').getValue() != '''') || $xcontext.action != ''view'')\u000a          &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.about'')&lt;/label&gt;&lt;/dt&gt;\u000a          &lt;dd #if($xcontext.action == ''view'')class="note"#end&gt;$doc.display(''comment'')&lt;/dd&gt;\u000a        #end\u000a      &lt;/dl&gt;\u000a      &lt;h1&gt;$msg.get(''platform.core.profile.section.contact'')&lt;/h1&gt;\u000a      &lt;dl&gt;\u000a        &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.email'')&lt;/label&gt;&lt;/dt&gt;\u000a        &lt;dd #if($xcontext.action=="view")class="email"#end&gt;#if($xcontext.action != ''view'' || !$obfuscateEmail)$doc.display(''email'')#else $doc.display(''email'').replaceAll(''@.*'', ''@ ~-~-~-~-~-~-'')#end&lt;/dd&gt;\u000a        &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.phone'')&lt;/label&gt;&lt;/dt&gt;\u000a        &lt;dd #if($xcontext.action == ''view'')class="tel"#end&gt;$doc.display(''phone'')&lt;/dd&gt;\u000a        &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.address'')&lt;/label&gt;&lt;/dt&gt;\u000a        &lt;dd #if($xcontext.action == ''view'')class="adr"#end&gt;$doc.display(''address'')&lt;/dd&gt;\u000a      &lt;/dl&gt;\u000a      #set($hasBlog = ($obj.getProperty(''blog'') &amp;&amp; $obj.getProperty(''blog'').getValue() != ''''))\u000a      #set($hasBlogFeed = ($obj.getProperty(''blogfeed'') &amp;&amp; $obj.getProperty(''blogfeed'').getValue() != ''''))\u000a      #if($hasBlog || $hasBlogFeed || $xcontext.action != ''view'')\u000a       &lt;h1&gt;$msg.get(''platform.core.profile.section.links'')&lt;/h1&gt;\u000a        &lt;dl&gt;\u000a          #if($hasBlog || $xcontext.action != ''view'')\u000a            &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.blog'')&lt;/label&gt;&lt;/dt&gt;\u000a            &lt;dd&gt;$doc.display(''blog'')&lt;/dd&gt;\u000a          #end\u000a          #if($hasBlogFeed || $xcontext.action != ''view'')\u000a            &lt;dt class="label"&gt;&lt;label&gt;$msg.get(''platform.core.profile.blogFeed'')&lt;/label&gt;&lt;/dt&gt;\u000a            &lt;dd&gt;$doc.display(''blogfeed'')&lt;/dd&gt;\u000a          #end\u000a        &lt;/dl&gt;\u000a      #end\u000a    &lt;/div&gt;\u000a  &lt;/div&gt;\u000a  &lt;div class="dashboardright"&gt;\u000a    #if ($xcontext.action == ''view'')\u000a      &lt;div class="userRecentChanges"&gt;\u000a        &lt;h1&gt;$msg.get(''platform.core.profile.section.recentChanges'')&lt;/h1&gt;\u000a{{/html}}\u000a#set ($rcAuthor = [$doc.fullName])\u000a#set ($rcChangesNb = 10)\u000a\u000a{{include document="Main.RecentChanges"/}}\u000a\u000a{{html clean="false"}}\u000a      &lt;/div&gt;\u000a    #end\u000a  &lt;/div&gt;\u000a  &lt;div class="dashboardspacer"&gt;&amp;nbsp;&lt;/div&gt;\u000a&lt;/div&gt;\u000a#if($xcontext.action == ''view'')\u000a  ## Close the vcard\u000a  &lt;/div&gt;\u000a#end\u000a{{/html}}##\u000a#end\u000a{{/velocity}}</content>\u000a</xwikidoc>\u000a')
INSERT INTO XWIKIRCS VALUES(237747783,1,1,'2010-05-21 14:29:24.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>XWiki</web>\u000a<name>TemplateProviderClass</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>XWiki.XWikiClasses</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1274448564000</creationDate>\u000a<date>1274448564000</date>\u000a<contentUpdateDate>1274448564000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title>Template Provider Class</title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/2.0</syntaxId>\u000a<hidden>false</hidden>\u000a<class>\u000a<name>XWiki.TemplateProviderClass</name>\u000a<customClass></customClass>\u000a<customMapping></customMapping>\u000a<defaultViewSheet></defaultViewSheet>\u000a<defaultEditSheet></defaultEditSheet>\u000a<defaultWeb></defaultWeb>\u000a<nameField></nameField>\u000a<validationScript></validationScript>\u000a<name>\u000a<name>name</name>\u000a<number>1</number>\u000a<picker>0</picker>\u000a<prettyName>name</prettyName>\u000a<size>30</size>\u000a<unmodifiable>0</unmodifiable>\u000a<validationMessage></validationMessage>\u000a<validationRegExp></validationRegExp>\u000a<classType>com.xpn.xwiki.objects.classes.StringClass</classType>\u000a</name>\u000a<spaces>\u000a<cache>0</cache>\u000a<classname></classname>\u000a<displayType>checkbox</displayType>\u000a<idField></idField>\u000a<multiSelect>1</multiSelect>\u000a<name>spaces</name>\u000a<number>3</number>\u000a<picker>0</picker>\u000a<prettyName>spaces</prettyName>\u000a<relationalStorage>0</relationalStorage>\u000a<separator> </separator>\u000a<separators></separators>\u000a<size>10</size>\u000a<sort>none</sort>\u000a<sql>select distinct doc.web from XWikiDocument doc order by doc.web</sql>\u000a<unmodifiable>0</unmodifiable>\u000a<validationMessage></validationMessage>\u000a<validationRegExp></validationRegExp>\u000a<valueField></valueField>\u000a<classType>com.xpn.xwiki.objects.classes.DBListClass</classType>\u000a</spaces>\u000a<template>\u000a<name>template</name>\u000a<number>2</number>\u000a<picker>0</picker>\u000a<prettyName>template</prettyName>\u000a<size>30</size>\u000a<unmodifiable>0</unmodifiable>\u000a<validationMessage></validationMessage>\u000a<validationRegExp></validationRegExp>\u000a<classType>com.xpn.xwiki.objects.classes.StringClass</classType>\u000a</template>\u000a</class>\u000a<content>{{include document="XWiki.ClassSheet"/}}\u000a</content></xwikidoc>')
INSERT INTO XWIKIRCS VALUES(244886082,1,1,'2010-03-01 16:19:25.000000000','','XWiki.Admin',FALSE,'<?xml version="1.0" encoding="UTF-8"?>\u000a\u000a<xwikidoc>\u000a<web>XWiki</web>\u000a<name>AdminGroupsSheet</name>\u000a<language></language>\u000a<defaultLanguage>en</defaultLanguage>\u000a<translation>0</translation>\u000a<parent>XWiki.AdminSheet</parent>\u000a<creator>XWiki.Admin</creator>\u000a<author>XWiki.Admin</author>\u000a<customClass></customClass>\u000a<contentAuthor>XWiki.Admin</contentAuthor>\u000a<creationDate>1211800831000</creationDate>\u000a<date>1267456765000</date>\u000a<contentUpdateDate>1267456765000</contentUpdateDate>\u000a<version>1.1</version>\u000a<title>AdminGroupsSheet</title>\u000a<template></template>\u000a<defaultTemplate></defaultTemplate>\u000a<validationScript></validationScript>\u000a<comment></comment>\u000a<minorEdit>false</minorEdit>\u000a<syntaxId>xwiki/1.0</syntaxId>\u000a<hidden>false</hidden>\u000a<content>### Globally administrate the groups in a wiki.\u000a#set($formname = "update")\u000a#set($saveaction = "save")\u000a## \u000a## Inject needed JS and CSS files\u000a##\u000a#set($ok = $xwiki.jsfx.use("js/xwiki/lightbox/lightbox.js", true))\u000a#set($ok = $xwiki.jsfx.use("js/xwiki/usersandgroups/usersandgroups.js", true))  \u000a#set($ok = $xwiki.ssfx.use("js/xwiki/usersandgroups/usersandgroups.css", true))\u000a#set($ok = $xwiki.jsfx.use("js/xwiki/table/livetable.js", true))\u000a#set($ok = $xwiki.ssfx.use("js/xwiki/table/livetable.css", true))\u000a##\u000a## url to get the groups for displaying them in the ajax-based table\u000a##\u000a#set($url = $doc.getURL(''get'', ''xpage=getgroups''))\u000a#if($xcontext.database != "xwiki") \u000a  #set($mainwk = false) \u000a#else \u000a  #set($mainwk = true) \u000a#end\u000a&lt;div id="xwikieditcontent"&gt;\u000a  &lt;table id="groupstable" class="xwiki-livetable"&gt;\u000a    &lt;tr&gt;\u000a      &lt;td class="xwiki-livetable-pagination" colspan="2"&gt;\u000a        &lt;span id="groupstable-limits" class="xwiki-livetable-limits"&gt;&lt;/span&gt;\u000a        &lt;span id="groupstable-ajax-loader" class="xwiki-livetable-loader"&gt;&lt;img src="$xwiki.getSkinFile(''icons/ajax-loader.gif'')" alt="$msg.get(''xe.livetable.loading'')" title="" /&gt;$msg.get("xe.livetable.loading")&lt;/span&gt;\u000a        &lt;span class="pagination"&gt;\u000a          &lt;span id="groupstable-pagination-text" class="xwiki-livetable-pagination-text"&gt;$msg.get("xe.pagination.page")&lt;/span&gt;\u000a          &lt;span id="groupstable-pagination" class="xwiki-livetable-pagination-content" &gt;&lt;/span&gt;\u000a        &lt;/span&gt;\u000a      &lt;/td&gt;\u000a    &lt;/tr&gt;\u000a    &lt;tr&gt;\u000a      &lt;td class="xwiki-livetable-display-container"&gt;\u000a        &lt;table class="xwiki-livetable-display"&gt;\u000a          &lt;thead class="xwiki-livetable-display-header"&gt;\u000a             &lt;tr&gt;\u000a               &lt;th class="xwiki-livetable-display-header-text" scope="col"&gt;$msg.get("xe.admin.groups.groupname")&lt;/th&gt;\u000a               &lt;th class="xwiki-livetable-display-header-text" scope="col"&gt;$msg.get("xe.admin.groups.members")&lt;/th&gt;\u000a               &lt;th class="xwiki-livetable-display-header-text" scope="col"&gt;$msg.get("xe.admin.groups.manage")&lt;/th&gt;\u000a             &lt;/tr&gt;\u000a             &lt;tr id="groupstable-filters" class="xwiki-livetable-display-filters"&gt;\u000a               &lt;td class="xwiki-livetable-display-header-filter"&gt;&lt;label for="name" class="hidden"&gt;$msg.get("xe.admin.groups.filter.groupName")&lt;/label&gt;&lt;input id="name" name="name" type="text" class="filter"/&gt;&lt;/td&gt;\u000a               &lt;td class="xwiki-livetable-display-header-filter"&gt;&lt;/td&gt;\u000a               &lt;td class="xwiki-livetable-display-header-filter"&gt;\u000a               #if(!$mainwk) \u000a                 &lt;label for="wiki" class="hidden"&gt;$msg.get("xe.admin.groups.filter.scope")&lt;/label&gt;\u000a                 &lt;select name="wiki" class="filter"&gt;\u000a                   &lt;option value="local" selected="selected"&gt;$msg.get("xe.admin.groups.local")&lt;/option&gt;\u000a                   &lt;option value="global"&gt;$msg.get("xe.admin.groups.global")&lt;/option&gt;\u000a                   &lt;option value="both"&gt;$msg.get("xe.admin.groups.both")&lt;/option&gt;\u000a                 &lt;/select&gt;\u000a               #else \u000a                 &lt;input type="hidden" value="local" name="wiki" /&gt; \u000a               #end\u000a               &lt;/td&gt;\u000a             &lt;/tr&gt;\u000a          &lt;/thead&gt;\u000a          &lt;tbody id="groupstable-display" class="xwiki-livetable-display-body"&gt;&lt;tr&gt;&lt;td&gt;&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;\u000a        &lt;/table&gt;\u000a      &lt;/td&gt;\u000a   &lt;/tr&gt;\u000a   &lt;tr&gt;\u000a     &lt;td id="tdAddNewUserOrGroup"&gt;\u000a       &lt;span class="buttonwrapper"&gt;&lt;a href="#" id="addNewGroup"&gt;$msg.get("rightsmanager.addnewgroup")&lt;/a&gt;&lt;/span&gt;\u000a     &lt;/td&gt;\u000a   &lt;/tr&gt;\u000a  &lt;/table&gt;\u000a  #set($addnewurl = $xwiki.getURL("XWiki.XWikiGroups", "view", "xpage=addnewgroup"))\u000a  #set($saveurl = $xwiki.getURL("XWiki.XWikiGroups", "view"))\u000a  #set($redirecturl = $xwiki.getDocument("XWiki.XWikiPreferences").getURL("admin", "editor=${editor}&amp;section=Groups")) \u000a  {pre}\u000a  &lt;script type="text/javascript"&gt;\u000a  //&lt;![CDATA[\u000a    (function() {\u000a      var startup = function() {\u000a        var callback = function(row, i, table) { return displayGroups(row, i, table, "$!{services.csrf.getToken()}"); };\u000a        var ta = new XWiki.widgets.LiveTable("$url", "groupstable", callback);\u000a        Event.observe($(''addNewGroup''), "click", makeAddHandler("$addnewurl", "$saveurl", "$redirecturl"));\u000a      }\u000a      if ((typeof(XWiki) != ''undefined'') &amp;&amp; (typeof(XWiki.widgets) != ''undefined'') &amp;&amp; (typeof(XWiki.widgets.LiveTable) != ''undefined'')) {\u000a        startup();\u000a      } else {\u000a        document.observe(''xwiki:dom:loading'', startup);\u000a      }\u000a    })();\u000a  //]]&gt;\u000a  &lt;/script&gt;\u000a  {/pre}\u000a&lt;/div&gt; ## xwikieditcontent\u000a</content></xwikidoc>')
